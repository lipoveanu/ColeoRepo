@{
    ViewBag.Title = "Project Statuses ";
}
@Styles.Render("~/Content/bootstrap-toggle.min.css")
@Scripts.Render("~/Scripts/bootstrap-toggle.min.js")

@Scripts.Render("~/Scripts/js-ProjectStatus.js")

<div class="row">
    <div id="index-projectstatus" class="col-md-4">
        @{Html.RenderAction("List");}
    </div>


    <div id="edit-projectstatus" class="col-md-7 pull-right">

    </div>
</div>

<style>
    [data-notify="container"]{
        width: 15%;
    }

</style>

<script>

    $(document).ready(function () {
        //set up notifications defaults
        NotificationDefaults();

        urlEdit = '@Url.Action("Edit", "ProjectStatus")';
        urlIndex = '@Url.Action("Index", "ProjectStatus")';
        urlList = '@Url.Action("List", "ProjectStatus")';
        urlDelete = '@Url.Action("Delete", "ProjectStatus")';
        
    });

</script>


